`ag_ini( IniAgent("/etc/powertweak/tweaks",
    $[
    "options" : [ "line_can_continue", "global_values", "join_multiline", "comments_last", "flat"],
    "comments": [ "^[ \t]*#.*$", "#.*", "^[ \t]*$", ],
    "params" : [
        $[
	    "match" : [ "([a-zA-Z0-9_:/-]+)[ \t]*=[ \t]*\"([^\"]*)\"", "%s = %s" ],
	"multiline" : [ "([a-zA-Z0-9_:/-]+)[ \t]*=[ \t]*\"([^\"]*)", "([^\"]*)\"", ],
        ],
        $[
	    "match" : [ "([a-zA-Z0-9_:/-]+)[ \t]*=[ \t]*([^\"]*[^ \t\"]|)[ \t]*$", "%s = %s",],
        ],
    ],
])
)

