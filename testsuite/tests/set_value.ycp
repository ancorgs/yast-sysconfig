DUMP("Testing list of changed values");

// set original value => no change
TEST( ``(Sysconfig::set_value("VARIABLE$/etc/sysconfig/test", "yes", true, false) ), [ READ, $[], $[] ], nil );
TEST( ``(Sysconfig::Modified() ), [ READ, $[], $[] ], nil );
TEST( ``(Sysconfig::get_modified() ), [ READ, $[], $[] ], nil );

// set new value
TEST( ``(Sysconfig::set_value("VARIABLE$/etc/sysconfig/test", "0777", false, false) ), [ READ, $[], $[] ], nil );
TEST( ``(Sysconfig::Modified() ), [ READ, $[], $[] ], nil );
TEST( ``(Sysconfig::get_modified() ), [ READ, $[], $[] ], nil );

