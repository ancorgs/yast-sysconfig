include "testsuite.ycp";

import "Sysconfig";

DUMP("Testing data type validation");

// initialize variable
TEST( ``(Sysconfig::Set([ $[ "sysconfig_key" : "VARIABLE",
    "sysconfig_path" : "/etc/sysconfig/test",
    "sysconfig_value" : "yes" ] ]) ), [ $[], $[], $[] ], nil );

// variable with metadata
map READ = $[ "syseditor" : $[ "value" : $[ "/etc/sysconfig/test" : $[ "VARIABLE" : "yes" ]],
			       "value_comment" : $[ "/etc/sysconfig/test" : $[ "VARIABLE" : "## Type: yesno\n# Comment with metadata" ]]
			    ]
];

DUMP("Test yesno type");
TEST( ``(Sysconfig::set_value("VARIABLE$/etc/sysconfig/test", "no", false, false) ), [ READ, $[], $[] ], nil );
TEST( ``(Sysconfig::set_value("VARIABLE$/etc/sysconfig/test", "yes", false, false) ), [ READ, $[], $[] ], nil );
TEST( ``(Sysconfig::set_value("VARIABLE$/etc/sysconfig/test", "123", false, false) ), [ READ, $[], $[] ], nil );
TEST( ``(Sysconfig::set_value("VARIABLE$/etc/sysconfig/test", "yesno", false, false) ), [ READ, $[], $[] ], nil );

TEST( ``(Sysconfig::get_description("VARIABLE$/etc/sysconfig/test") ), [ READ, $[], $[] ], nil );

