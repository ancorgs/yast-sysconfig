DUMP("Testing value setting");

// get current value (should be 023 frou previous tests)
TEST( ``(Sysconfig::get_description("VARIABLE$/etc/sysconfig/test") ), [ READ, $[], $[] ], nil );
// force wrong value
TEST( ``(Sysconfig::set_value("VARIABLE$/etc/sysconfig/test", "0897", true, false) ), [ READ, $[], $[] ], nil );
